<div class="card">
  <div class="card-header">
    <h1>edit</h1>
  </div>
  <div class="card-body">
    <h5 class="card-title">
      <%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
<br>
  <%= f.label :details %>
  <%= f.text_field :details %>
<br>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
<br>
  <%= f.submit %>
<% end %>
    </h5>

  </div>
</div>
